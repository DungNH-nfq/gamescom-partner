{
  "name": "gamescom-partner",
  "private": true,
  "version": "0.1.0",
  "description": "This is the app for Gamescom partners for candidate testing.",
  "author": "Dzung <d.nguyenhoang@shopmacher.de>",
  "license": "UNLICENSED",
  "keywords": [
    "react",
    "vitejs",
    "typescript",
    "tailwindcss"
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "build-staging": "tsc && vite build --mode staging",
    "preview": "vite preview",
    "predeploy": "npm run build-staging",
    "deploy": "gh-pages -d dist",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.tsx\"",
    "test": "jest --coverage"
  },
  "dependencies": {
    "dotenv": "^16.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.2",
    "@testing-library/react": "^12.1.3",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.4.0",
    "@types/node": "^17.0.18",
    "@types/react": "^17.0.33",
    "@types/react-dom": "^17.0.10",
    "@vitejs/plugin-react": "^1.0.7",
    "autoprefixer": "^10.4.2",
    "gh-pages": "^3.2.3",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^27.5.1",
    "postcss": "^8.4.6",
    "prettier": "^2.5.1",
    "tailwindcss": "^3.0.22",
    "ts-jest": "^27.1.3",
    "ts-node": "^10.5.0",
    "typescript": "^4.5.4",
    "vite": "^2.8.0"
  }
}
